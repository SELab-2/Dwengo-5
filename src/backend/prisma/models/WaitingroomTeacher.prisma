model WaitingroomTeacher {
  teachers_id Int
  classes_id  Int
  teacher     Teacher @relation(fields: [teachers_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  class       Class   @relation(fields: [classes_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([teachers_id, classes_id])
}
