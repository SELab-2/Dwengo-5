model StudentLearningObject {
  students_id          Int
  learning_objects__id String         @db.VarChar
  learning_objects     LearningObject @relation(fields: [learning_objects__id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  students             Student        @relation(fields: [students_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([students_id, learning_objects__id])
}
