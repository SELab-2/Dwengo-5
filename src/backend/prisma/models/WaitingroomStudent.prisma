model WaitingroomStudent {
  students_id Int
  classes_id  Int
  student     Student @relation(fields: [students_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  class       Class   @relation(fields: [classes_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([classes_id, students_id])
}
