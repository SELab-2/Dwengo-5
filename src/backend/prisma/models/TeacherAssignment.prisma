model TeacherAssignment {
  teachers_id    Int
  assignments_id Int
  assignments    Assignment @relation(fields: [assignments_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  teachers       Teacher    @relation(fields: [teachers_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([teachers_id, assignments_id])
}
