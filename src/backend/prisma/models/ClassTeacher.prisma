model ClassTeacher {
  classes_id  Int
  teachers_id Int
  classes     Class   @relation(fields: [classes_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  teachers    Teacher @relation(fields: [teachers_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([classes_id, teachers_id])
}
